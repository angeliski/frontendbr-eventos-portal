{
  "name": "eventos-dashboard",
  "description": "Painel para aprovação de eventos",
  "version": "0.1.0",
  "scripts": {
    "start": "react-app-rewired start",
    "build": "react-app-rewired build",
    "test": "react-app-rewired test",
    "eject": "react-scripts eject",
    "lint": "node_modules/.bin/eslint src/*"
  },
  "dependencies": {
    "axios": "^0.19.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-icons": "^2.2.7",
    "react-router-dom": "^5.0.1",
    "react-scripts": "^3.0.1",
    "styled-components": "^4.3.2"
  },
  "devDependencies": {
    "eslint-config-divisio": "^0.2.0",
    "husky": "^3.0.2",
    "lint-staged": "^9.2.1",
    "react-app-rewire-aliases": "^0.2.0",
    "react-app-rewired": "^2.1.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "flow check && lint-staged"
    }
  },
  "lint-staged": {
    "linters": {
      "*.js": [
        "eslint",
        "git add"
      ]
    }
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
